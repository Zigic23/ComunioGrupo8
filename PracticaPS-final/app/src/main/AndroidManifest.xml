<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pdred.practicaps_final" >

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/unnamed"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".Login.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Equipo.EquipoActivity" />
        <activity
            android:name=".Main_Menu.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".Perfil.MiPerfil" >
        </activity>
        <activity android:name=".Liga_Comunidad.MiLiga" >
        </activity>
        <activity android:name=".Mercado.Mercado" >
        </activity>
        <activity android:name=".Registro.Registro" >
        </activity>
        <activity android:name=".Perfil.mi_perfil2" >
        </activity>
        <activity android:name=".Liga_Comunidad.mi_liga2" >
        </activity>
        <activity android:name=".Mercado.mercado2" >
        </activity>
    </application>

</manifest>
